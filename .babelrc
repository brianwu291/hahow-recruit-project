{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react"
  ],
  "env": {
    "production": {
      "presets": ["minify"]
    }
  },
  "plugins": [
    [
      "@babel/plugin-proposal-class-properties"
    ],
    [ 
      "babel-plugin-styled-components",
      {
        "displayName": true
      }
    ]
  ]
}